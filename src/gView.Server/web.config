<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="%LAUNCHER_PATH%" stdoutLogEnabled="false" hostingModel="InProcess">
      <environmentVariables>
        <environmentVariable name="ASPNETCORE_HTTPS_PORT" value="44331" />
        <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Development" />
        <environmentVariable name="COMPLUS_ForceENC" value="1" />
        <environmentVariable name="GDAL_DRIVER_PATH" value="./gdalplugins" />
      </environmentVariables>
    </aspNetCore>
	<security>
		<requestFiltering>
			<!-- Handle requests up to 1 GB -->
			<requestLimits maxAllowedContentLength="1073741824" />
		</requestFiltering>
	</security>
  </system.webServer>
</configuration>